<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.caipudaquandemo.mapper.UserManagerMapper">
    <select id="getUserManagerByMassage" resultType="com.caipudaquandemo.entity.UserManager">
        SELECT * FROM user_info WHERE username =#{username} AND password=#{password} AND per=1
    </select>
</mapper>